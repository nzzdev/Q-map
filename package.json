{
  "name": "@nzz/q-map",
  "version": "1.10.6",
  "description": "Q map tool",
  "main": "index.js",
  "scripts": {
    "test": "lab -a @hapi/code -c -P tests --verbose --leaks",
    "build": "rm -rf scripts && mkdir scripts && rm -rf styles && mkdir styles && rm -rf resources/fixtures && mkdir -p resources/fixtures/data && node tasks/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nzzdev/Q-map.git"
  },
  "author": "Beni Buess <beni.buess@nzz.ch>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nzzdev/Q-map/issues"
  },
  "homepage": "https://github.com/nzzdev/Q-map#readme",
  "dependencies": {
    "@hapi/boom": "^7.4.2",
    "@hapi/hapi": "^19.0.0",
    "@hapi/inert": "^5.2.1",
    "@hapi/joi": "^15.1.0",
    "@turf/turf": "^5.1.6",
    "ajv": "^6.10.2",
    "babel-plugin-import": "^1.12.0",
    "clone": "^2.1.2",
    "svelte": "^2.16.1",
    "systemjs": "^3.1.6",
    "systemjs-builder": "^0.16.15"
  },
  "jspm": {
    "name": "q-map",
    "dependencies": {
      "babel": "npm:babel-core@^6.22.1",
      "leaflet": "npm:leaflet@1.2.0",
      "leaflet-minimap": "npm:leaflet-minimap@^3.4.0",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.20",
      "text": "github:systemjs/plugin-text@^0.0.9"
    },
    "devDependencies": {},
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "overrides": {
      "npm:debug@2.6.9": {
        "main": "src/browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./src/browser.js": {
            "node": "./src/node.js"
          },
          "./node.js": {
            "browser": "./src/browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.4": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:leaflet@1.2.0": {
        "jspmNodeConversion": false
      },
      "npm:lodash@4.17.15": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:pbkdf2@3.0.17": {
        "main": "browser.js"
      },
      "npm:safe-buffer@5.1.2": {
        "browser": "index.js"
      }
    }
  },
  "devDependencies": {
    "@hapi/code": "^5.3.1",
    "@hapi/lab": "^19.0.0",
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "escape-json-node": "^2.0.0",
    "glob": "^7.1.4",
    "jsdom": "^15.1.1",
    "jspm": "^0.17.0-beta.49",
    "postcss": "^7.0.17",
    "postcss-import": "^12.0.1",
    "sass": "^1.22.7"
  }
}
